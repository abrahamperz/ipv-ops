# Google Sheets Configuration
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_CREDENTIALS_FILE=credentials.json

# Application Settings
ENV=development
PORT=8000

# Airtable Configuration
# Get your API key from: https://airtable.com/account
AIRTABLE_API_KEY=your_api_key_here

# The ID of your Airtable base (found in the URL: https://airtable.com/appXXXXXXXXXXXXX/...)
AIRTABLE_BASE_ID=your_base_id_here

# The name of the table you want to access
AIRTABLE_TABLE_NAME=your_table_name_here

# Note: Copy this file to .env and replace the placeholder values with your actual credentials.
# Make sure to add .env to your .gitignore file to keep your credentials secure.
